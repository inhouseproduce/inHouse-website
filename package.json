    "nodemon": "^2.0.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1"
  }
}
